@model List<RestaurantAllViewModel>

@{
    ViewData["Title"] = "My Restaurants";
}

<h3 class="text-center">@ViewData["Title"]</h3>
<hr/>

@if (!Model.Any())
{
    <h2 class="text-center">You dont have any restaurants!</h2>
}
else
{
    <div class = "row">
        @foreach (RestaurantAllViewModel restaurant in Model)
        {
            <partial name = "_RestaurantPartial" model="restaurant" />
        }
    </div>
}